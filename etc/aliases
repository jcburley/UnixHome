#!/bin/sh

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias lt='ls -lt'
alias ltd='ls -ltd'
alias ll='ls -l'
alias lld='ls -ld'
alias lta='ls -ltA'
alias ltad='ls -ltAd'
alias lla='ls -lA'
alias llad='ls -lAd'
alias l1='ls -1'
alias l1d='ls -1d'
alias l1a='ls -1A'
alias l1ad='ls -1Ad'

alias gitdown='git pull; git fetch -p'
alias gitstat='git status -unormal'
alias gitnbr='git checkout -b'
alias gitmast='git checkout master'
alias gitdiff='git diff'
